<h3 class="page-center">ブックマークした便利商品一覧</h3>
<div class="container-fluid">
  <div class="row">
    <% @bookmarks.each do |bookmark| %>
      <div class='col-md-6 col-lg-4 pt-3'>
        <div class="card-group h-100">
          <div class="card">
            <div class="card-swiper-between">
              <%= render 'products/swiper', product: bookmark.product %>
            </div>
            <div class="card-body">
              <%= render 'products/card', product: bookmark.product %>
              
            <!--</div>-->
              
            <%= render 'products/card_body', product: bookmark.product %>
          </div>
        </div>
      </div>
    <% end %>
  </div>
  <div class="text-center mt-3 mb-3 ">
    <%= paginate @bookmarks %>
  </div>
</div>

