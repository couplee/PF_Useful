<h3 class="page-center">いいねした便利商品一覧</h3>
<div class="container-fluid">
  <div class="row">
    <% @likes.each do |like| %>
      <div class='col-md-6 col-lg-4  pt-3'>
        <div class="card-group h-100">
          <div class="card">
            <%= render 'products/swiper', product: like.product %>
            <div class="card-body">
              <%= render 'products/card', product: like.product %>
              
            <!--</div>-->
            
            <%= render 'products/card_body', product: like.product %>
          </div>
        </div>
      </div>
    <% end %>
  </div>
  <div class="text-center mt-3 mb-3 ">
    <%= paginate @likes %>
  </div>
</div>
